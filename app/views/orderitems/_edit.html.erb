<%= form_with scope: :order_item, url: edit_orderitem(order_item.id), method: :patch do |f| %>
  <% raise %>
  <%= f.hidden_field :order_id %>
  <%= f.hidden_field :product_id %>
  
  <%= @product_name %>

  <%= f.label :quantity %>
  <%= f.text_field :quantity %>

  <%= f.submit "Update Cart Item" %>
<% end %>
