    
<%= form_with model: @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :category %>
  <%= f.select :category, ['NSFW', 'SFW', 'Messy', 'Clean', 'Edible'] %>

  <%= f.label :stock %>
  <%= f.text_field :stock %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.submit "Submit" %>
<% end %>